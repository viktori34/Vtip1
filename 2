my = ('Вика Полушкова 19')
people = [my]
my1 = ('Аня Соколова 11')
my2 = ('Пуся МУзова 15')
my3 = ('Евгеша Мусин 26')
people = [my, my1, my2, my3]
print(people)
people.sort()
print(people)
# ['Вика Полушкова 19', 'Аня Соколова 11', 'Пуся МУзова 15', 'Евгеша Мусин 26']
# ['Аня Соколова 11', 'Вика Полушкова 19', 'Евгеша Мусин 26', 'Пуся МУзова 15']
