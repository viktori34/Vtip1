
s = str('''But, soft, behold! Lo, where it comes again!''')
p=s.split()
print(p)
ch = {}

dict((i, p.count(i)) for i in p)
print(dict((i, p.count(i)) for i in p))
#['But,', 'soft,', 'behold!', 'Lo,', 'where', 'it', 'comes', 'again!']
