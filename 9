#'waving': 1, 'boughs': 1, 'storm,': 1, 'new': 1, 'me': 2, 'old.': 1, 'It': 1, 'takes': 1, 'by': 1, 'surprise,': 1, 'yet': 1, 'unknown.': 1, 'Its': 1, 'effect': 1,
